{
    "name": "Redirecting Fallbacks",
    "description": "The redirecting-fallbacks project aims to provide an standardized way to configure your http redirects. e.g.: instead of showing an 404 page, redirect to the start page.",
    "type": "library",
    "keywords": ["redirects", "redirecting", "redirect", "404", "fallback"],
    "license": "BSD-3",
    "authors": [
        {
            "name": "Anton Sylvester Zoffmann",
            "email": "anton_sylvester@msn.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://github.com/jidoka1902/redirecting-fallbacks.git"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "Jidoka1902\\RedirectingFallbacks\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jidoka1902\\RedirectingFallbacks\\Tests": "test/"
        }
    }
}
